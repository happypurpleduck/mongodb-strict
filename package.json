{
	"name": "@purpleduck/mongodb-strict",
	"type": "module",
	"version": "0.0.0",
	"exports": {
		".": {
			"default": "./src/index.ts"
		},
		"./aggregate": {
			"default": "./src/aggregate.ts"
		},
		"./collection": {
			"default": "./src/collection.ts"
		},
		"./document": {
			"default": "./src/document.ts"
		},
		"./*": {
			"default": "./src/types/*"
		}
	},
	"module": "src/index.ts",
	"files": ["./src/*.ts", "./src/types/*.ts"],
	"scripts": {
		"check": "tsc --noEmit",
		"test": "vitest --typecheck",
		"lint": "eslint src --fix"
	},
	"peerDependencies": {
		"mongodb": "^6.13.0",
		"type-fest": "^4.34.1",
		"typescript": "^5.9.2"
	},
	"dependencies": {},
	"devDependencies": {
		"@antfu/eslint-config": "^5.2.1",
		"@biomejs/biome": "^2.2.2",
		"@faker-js/faker": "^10.0.0",
		"@types/bun": "latest",
		"eslint": "^9.34.0",
		"vitest": "^3.2.4"
	},
	"trustedDependencies": ["@biomejs/biome", "esbuild", "unrs-resolver"]
}
