{
	"name": "@purpleduck/mongodb-strict",
	"version": "0.0.0",
	"module": "src/index.ts",
	"type": "module",
	"scripts": {
		"type-check": "tsc",
		"test": "vitest --typecheck"
	},
	"exports": {
		".": {
			"import": "./src/index.ts"
		},
		"./aggregate": {
			"import": "./src/aggregate.ts"
		},
		"./collection": {
			"import": "./src/collection.ts"
