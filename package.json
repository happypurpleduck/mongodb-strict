		}
	},
	"files": ["./src/*.ts", "./src/types/*.ts"],
	"devDependencies": {
		"@biomejs/biome": "^1.9.4",
		"@types/bun": "latest",
		"@faker-js/faker": "^9.5.0",
		"vitest": "^3.0.6"
	},
	"peerDependencies": {
		"typescript": "^5.0.0",
		"mongodb": "^6.13.0",
		"type-fest": "^4.34.1"
	},
	"dependencies": {},
	"trustedDependencies": ["@biomejs/biome"]
}
